[ req ]
default_bits = 2048
encrypt_key = no
prompt = no
default_md = sha256
distinguished_name = dn
req_extensions = req_v3

[ req_v3 ]
subjectAltName = @alt_names

[ dn ]
CN = example.com
organizationName = eng-team1
+organizationName = eng-team2

[ alt_names ]
IP.1 = 127.0.0.1
email = valid@example.com
